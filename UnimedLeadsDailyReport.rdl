  <DataSets>
    <DataSet Name="DataSetLeads">
      <Query>
        <DataSourceName>UnimedDataSource</DataSourceName>
        <CommandText>SELECT        

FilteredLead.owneridname
,FilteredLead.leadid
,FilteredLead.ownerid
,FilteredLead.statuscode
,FilteredLead.statuscodename
,t1.Leadid
,t1.ModifiedOn

FROM
FilteredLead 

INNER JOIN
FilteredSystemUser ON FilteredLead.ownerid = FilteredSystemUser.systemuserid


LEFT JOIN
(SELECT 
FilteredLead.leadid Leadid,
FilteredPhoneCall.modifiedon ModifiedOn
FROM FilteredLead
INNER JOIN 
FilteredPhoneCall
ON FilteredLead.leadid =  FilteredPhoneCall.regardingobjectid
WHERE FilteredPhoneCall.statecode = 2
AND (CONVERT(date, FilteredPhoneCall.modifiedon) = CONVERT(date, GETDATE()))
) as t1
ON FilteredLead.leadid = t1.leadid


WHERE        (FilteredLead.statecode = 0) 
AND (FilteredSystemUser.isdisabled = 0)
AND FilteredSystemUser.new_mainteamname = 'Sales'

/*GROUP BY FilteredLead.ownerid, FilteredLead.owneridname,FilteredLead.statuscode, FilteredLead.statuscodename,FilteredLead.leadid*/</CommandText>
        <Timeout>30</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
